<%= link_to '一覧画面へ', '/users/new' %>
<% @users.each do |user| %>
<p> 
   <%= user.name %>
   <%= user.age %>
   <%= link_to "編集", "/users/#{user.id}/edit", method: :get, data: {confirm:"よろしいですか？"} %>
   <%= link_to "削除", "/users/#{user.id}", method: :delete, data: {confirm:"削除しますか？"} %>
 </p>
<% end %>